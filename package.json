{
  "name": "kc-tooltip",
  "version": "0.1.6",
  "description": "Component for easily showing customizable tooltips",
  "main": "lib/index.js",
  "scripts": {
    "start": "parcel src/docs/index.html -d docs",
    "build-docs": "parcel build src/docs/index.html -d docs",
    "build-lib": "parcel build src/lib/index.js -d lib",
    "build": "npm run build-docs && npm run build-lib",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d docs"
  },
  "keywords": [
    "react",
    "tooltip"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "parcel-bundler": "^1.8.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "author": "KingCosmic <KingCosmicDev@gmail.com>",
  "dependencies": {
    "prop-types": "^15.6.1"
  }
}
